<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraint"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

        <ImageButton
            android:id="@+id/CameraBut"
            android:layout_width="188dp"
            android:layout_height="69dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="12dp"
            android:contentDescription="@string/title_activity_camera"
            android:drawableTop="@drawable/ic_baseline_camera_alt_24"
            android:src="@drawable/ic_baseline_camera_alt_24"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:src="@drawable/ic_baseline_camera_alt_24" />

        <ImageButton
            android:id="@+id/LockBut"
            android:layout_width="188dp"
            android:layout_height="69dp"
            android:layout_marginTop="12dp"
                android:layout_marginEnd="11dp"
            android:layout_marginRight="11dp"
            android:contentDescription="@string/title_activity_lock"
            android:drawableTop="@drawable/ic_baseline_lock_24"
            android:src="@drawable/ic_baseline_lock_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/CameraBut"
            app:layout_constraintTop_toTopOf="parent" />

        <FrameLayout
            android:id="@+id/GenFragment"
            android:layout_width="373dp"
            android:layout_height="729dp"
            android:layout_marginStart="9dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="86dp"
            android:layout_marginEnd="11dp"
            android:layout_marginRight="11dp"
            android:layout_marginBottom="14dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
